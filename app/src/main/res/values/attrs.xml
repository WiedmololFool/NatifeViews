<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RectangleView">
        <attr name="roundRadius" format="dimension"/>
        <attr name="lineWidth" format="dimension"/>
        <attr name="lineColor" format="reference|color"/>
    </declare-styleable>

    <declare-styleable name="LinearView">
        <attr name="itemTextSize" format="dimension"/>
        <attr name="itemTextColor" format="reference|color"/>
        <attr name="itemBackgroundColor" format="reference|color"/>
        <attr name="itemPadding" format="dimension"/>
    </declare-styleable>


</resources>